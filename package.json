{
  "name": "@antfu/nip",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.9.0",
  "description": "A better pnpm install xxx with prompts and catalogs support",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/nip#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/nip.git"
  },
  "bugs": "https://github.com/antfu/nip/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "nip": "./bin/nip.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/ni": "catalog:prod",
    "@clack/prompts": "catalog:prod",
    "ansis": "catalog:prod",
    "cac": "catalog:prod",
    "fast-npm-meta": "catalog:prod",
    "find-up": "catalog:prod",
    "package-manager-detector": "catalog:prod",
    "pnpm-workspace-yaml": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@antfu/utils": "catalog:inlined",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "eslint": "catalog:cli",
    "lint-staged": "catalog:cli",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:utils",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "unbuild": "catalog:cli",
    "vite": "catalog:cli",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
